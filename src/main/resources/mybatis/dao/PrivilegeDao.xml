<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.springboot.online_bookstore_backend.repository.PrivilegeDao">
    <select id="selectAll" resultType="Privilege">
        select * from privilege
    </select>
    <select id="findById" parameterType="Long" resultType="Privilege">
        select * from privilege where priv_id = #{priv_id}
    </select>
</mapper>
